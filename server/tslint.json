{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier",
    "tslint-config-airbnb"
  ],
  "jsRules": {
    "max-line-length": {
      "options": [120]
    }
  },
  "rules": {
    "max-line-length": {
      "options": [120]
    },
    "object-literal-sort-keys": false,
    "quotemark": [true, "single"],
    "no-console": [false, "log", "error"]
  },
  "rulesDirectory": []
}